    "# Q.5622\n",
    "strlist = ['','','','ABC','DEF','GHI','JKL','MNO','PQRS','TUV','WXYZ','']\n",
    "\n",
    "alpha = input().upper()\n",
    "time = 0\n",
    "for check in alpha:\n",
    "    for s in strlist[3:]:\n",
    "        if check in s:\n",
    "            time += strlist.index(s)\n",
    "print(time)"
