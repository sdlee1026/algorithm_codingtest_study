    "T = int(input())\n",
    "S = [list( map(str, input().split()) ) for i in range(T)]\n",
    "for i, j in S:\n",
    "    print_str =''\n",
    "    for k in j:\n",
    "        print_str+=k*int(i)\n",
    "    print(print_str)"
