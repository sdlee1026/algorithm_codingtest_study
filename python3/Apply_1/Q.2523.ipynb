    "# Q.2523\n",
    "num = int(input())\n",
    "max_val = num *2\n",
    "\n",
    "for i in range(1,num*2+1):\n",
    "    if i<=num:\n",
    "        print('*'*i)\n",
    "    else:\n",
    "        print('*'*(max_val-i))"
