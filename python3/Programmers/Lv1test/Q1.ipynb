    "def solution(s):\n",
    "    # s < 문자열\n",
    "    p_count, y_count = 0, 0\n",
    "    s=s.lower()\n",
    "    for i in s:\n",
    "        if i == 'p': p_count+=1\n",
    "        elif i == 'y': y_count+=1\n",
    "    \n",
    "    if p_count == y_count : return True\n",
    "    else: return False\n",
    "solution('Pyy')"
   ]
