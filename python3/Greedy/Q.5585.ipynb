    "# Q.5585\n",
    "\n",
    "coin_list = [500,100,50,10,5,1]\n",
    "\n",
    "money = 1000-int(input())\n",
    "index = 0\n",
    "count = 0\n",
    "while True:\n",
    "    if money == 0 : break;\n",
    "    count += money//coin_list[index]\n",
    "    money %= coin_list[index]\n",
    "    index+=1\n",
    "print(count)"
   ]
